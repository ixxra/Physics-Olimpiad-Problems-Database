#usage: sed -r -f wikitableToCsv.sed 

/\{\||^\s*$/{
    d
}

/^\s*!/{
    s/^\s*!//
    H
    d
}

/\|-/{
   x
   s/\n/, /g
   s/^[^,]*,//
   b  
}

/\|\}/{
    
   x
   s/\n/, /g
   s/^[^,]*,//
   b
}

/^\s*\|[^-]?/{
    s/^\s*\|([^-]?)/\1/
    s/''([^']+)''/\1/g
    s/\{\{flag\|(.*)\}\}/\1/
    s/\[\[(.*)\]\]/\1/
    H
    d
}

